<script>
function showTab(tab) {
  document.querySelectorAll('.tab-btn').forEach(b => b.classList.remove('active'));
  event.target.classList.add('active');

  const allCards = document.querySelectorAll('.cmd-card');
  const allTitles = document.querySelectorAll('.category-title');
  
  allCards.forEach(c => {
    if (tab === 'all') {
      c.style.display = '';
    } else {
      c.style.display = c.dataset.section === tab ? '' : 'none';
    }
  });

  allTitles.forEach(t => {
    if (tab === 'all') {
      t.style.display = '';
    } else {
      // Map tab to category
      const map = {
        general: ['๐ฌ ุงูุฃูุงูุฑ ุงูุนุงูุฉ'],
        moderation: ['๐ก๏ธ ุฃูุงูุฑ ุงูุฅุดุฑุงู'],
        autoreply: ['๐ค ุงูุฑุฏ ุงูุชููุงุฆู'],
        protection: ['๐ ุฃูุงูุฑ ุงูุญูุงูุฉ'],
        levels: ['โญ ูุธุงู ุงููููู'],
        ticket: ['๐ซ ูุธุงู ุงูุชูุช'],
        utility: ['โ๏ธ ุฃุฏูุงุช ุฃุฎุฑู']
      };
      const show = map[tab] || [];
      t.style.display = show.some(s => t.textContent.includes(s.replace(/[^\w\u0600-\u06FF ]/g, ''))) ? '' : 'none';
    }
  });
}

function searchCommands() {
  const q = document.getElementById('searchInput').value.toLowerCase().trim();
  const cards = document.querySelectorAll('.cmd-card');
  let found = 0;

  cards.forEach(c => {
    const name = c.dataset.name || '';
    const desc = c.querySelector('.cmd-desc')?.textContent || '';
    const match = name.includes(q) || desc.includes(q) || q === '';
    c.style.display = match ? '' : 'none';
    if (match) found++;
  });

  document.getElementById('noResults').classList.toggle('show', found === 0 && q !== '');
  
  // Show all tabs when searching
  document.querySelectorAll('.category-title').forEach(t => t.style.display = '');
}
</script>
